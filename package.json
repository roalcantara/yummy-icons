{
  "name": "yummy-icons",
  "version": "0.1.4",
  "description": "Free yummy food font icons",
  "engines": {
    "node": "10",
    "yarn": ">=1.9.0 <2.0.0"
  },
  "main": "index.js",
  "scripts": {
    "typescript": "./node_modules/.bin/typescript",
    "prettier": "./node_modules/.bin/prettier",
    "tslint": "./node_modules/.bin/tslint",
    "tslint:prettier": "./node_modules/.bin/tslint-config-prettier-check",
    "pretty-quick": "./node_modules/.bin/pretty-quick",
    "gulp": "./node_modules/.bin/gulp",
    "lint:saas": "./node_modules/.bin/sass-lint -c ./.sass-lint.yml --verbose --no-exit",
    "format:write": "yarn prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint:check": "yarn tslint:prettier ./tslint.yml",
    "lint": "yarn lint:saas && yarn tslint -p tsconfig.json",
    "build": "tsc && yarn gulp build",
    "version:current": "echo $npm_package_version",
    "version:commit": "git commit -am \"chore(release): Bump version to $(npm run version:current --silent)\"",
    "version:major": "npm --no-git-tag-version version major && yarn version:commit",
    "version:minor": "npm --no-git-tag-version version minor && yarn version:commit",
    "version:patch": "npm --no-git-tag-version version patch && yarn version:commit",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "release:tag": "git tag -a `echo v$(npm run version:current --silent)`",
    "release": "yarn release:tag && git push && git push --tags",
    "release:patch": "yarn build && git add . && yarn version:patch && yarn release && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "roalcantara/yummy-icons"
  },
  "keywords": [
    "icon",
    "icons",
    "web-font"
  ],
  "author": "Rogério R. Alcântara <rogerio.alcantara@gmail.com> (http://github.com/roalcantara)",
  "license": "CC-BY-SA-4.0",
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "conventional-changelog-cli": "^2.0.23",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-pug": "^4.0.1",
    "gulp-sass": "^4.0.2",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "pug": "^2.0.4",
    "sass-lint": "^1.13.1",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS --config ./.commitlintrc.yml",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.*": [
      "yarn pretty-quick --staged",
      "git add"
    ],
    "*.{css,scss}": [
      "yarn sass-lint",
      "git add"
    ]
  }
}
